package com.tcs.training;

public class TestTypesVariables {

     public static void main(String[] args) {
		int a = 10;
		int b = 20;
		int c=0;
		if(a<10) {
			c= a+b;
		}
		if(a>=10) {
			c=a-b;
		}		
		System.out.println(c);
		
		boolean result = a>b && a>c;
		System.out.println(result);
		//false and true are boolean literals.
		result =false == false;
		System.out.println(result);
	}
}
